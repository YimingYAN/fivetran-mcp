{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.YimingYAN/fivetran-mcp",
  "description": "MCP server for Fivetran API - manage syncs, check status, and control data pipelines",
  "repository": {
    "url": "https://github.com/YimingYAN/fivetran-mcp",
    "source": "github"
  },
  "version": "0.3.1",
  "packages": [
    {
      "registryType": "pypi",
      "identifier": "fivetran-mcp",
      "version": "0.3.1",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "name": "FIVETRAN_API_KEY",
          "description": "Fivetran API key",
          "isRequired": true,
          "isSecret": true,
          "format": "string"
        },
        {
          "name": "FIVETRAN_API_SECRET",
          "description": "Fivetran API secret",
          "isRequired": true,
          "isSecret": true,
          "format": "string"
        }
      ]
    }
  ]
}
